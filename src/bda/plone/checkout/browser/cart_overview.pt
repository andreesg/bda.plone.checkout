 
<div id="cart"
     i18n:domain="bda.plone.cart" class="checkout final-checkout"
     tal:define="tickets python: '/tickets' in context.absolute_url()">


     <style type="text/css">
      div.tickets-breadcrumbs.original {
        display: none !important;
      }
     </style>



  <div class="tickets-breadcrumbs final-checkout" tal:condition="tickets">
      <a href="@@cart"
       class="btn step step-1" type="button">1. Tickets</a>

      <a href="javascript:history.back()"
       class="btn step step-2" type="button">2. Persoonsgegevens</a>

      <a href=""
       class="btn step step-3 selected" type="button">3. Gegevens controleren</a>

      <a href=""
       class="btn disabled step step-4" type="button">4. Betalen</a>
  </div>

  <h2 i18n:translate="">Winkelwagen</h2><br>
 

  <span style="display:none;"
        class="cart_execution_context">checkout</span>

  <div class="summary_item summary_item_tickets">
    <ul class="cart_item_content cart_item_content_tickets">
      <li i18n:translate="" class="price">Price</li>
      <li i18n:translate="" class="quantity">Amount</li>
      <li i18n:translate="" class="subtotal">Subtotal</li>
    </ul>
  </div>

  <span id="cart_item_template" style="display: none;"
    tal:define="currency view/currency">
    <div class="cart_item cart_overview">
      <h3 class="cart_item_title">Title</h3>
      <div class="cart_item_alert" style="display: none;">Alert</div>
      <img class="cart_item_preview_image" src="" />
      <p class="cart_item_description">Description</p>
      <p class="cart_item_comment_wrapper">
        <strong i18n:translate="comment">Comment</strong>:
        <span class="cart_item_comment">comment</span>
      </p>
      <ul class="cart_item_content">
        <li>
          <span class="price original">
            <span class="cart_item_original_price">Price</span>&nbsp;<span tal:content="currency">EUR</span>
          </span>
        </li>
        <li>
          <span class="cart_item_count">0</span>
          <!--<span class="cart_item_quantity_unit">Quantity</span>-->
        </li>
        <li>
          <span class="price">
            <!--<span i18n:translate="price">Price:</span>-->
            <span class="cart_item_price">Price</span> <span>â‚¬</span>
          </span>
        </li>
      </ul>
      <div class="visualClear"></div>
    </div>
  </span>

  <div id="cart_items" style="display: none;">
    <div id="cart_no_items" style="display: none;">
      <p i18n:translate="empty_cart">Cart is empty</p>
    </div>
  </div>

  <div id="cart_summary" style="display: none;" class="summary_wrapper">
    <table class="cart_sum_content"
           tal:define="currency view/currency">
      <tal:details condition="not:view/summary_total_only">
        <tr>
          <td i18n:translate="summary_net">Net:</td>
          <td class="cart_net">0.00</td>
          <td class="currency" tal:content="currency" tal:condition="currency">EUR</td>
        </tr>
        <tr>
          <td i18n:translate="summary_vat">VAT.:</td>
          <td class="cart_vat">0.00</td>
          <td class="currency" tal:content="currency" tal:condition="currency">EUR</td>
        </tr>
        <tr class="discount" style="display: none;">
          <td i18n:translate="discount_net">Discount Net:</td>
          <td class="discount_net">0.00</td>
          <td class="currency" tal:content="currency" tal:condition="currency">EUR</td>
        </tr>
        <tr class="discount" style="display: none;">
          <td i18n:translate="discount_vat">Discount VAT.:</td>
          <td class="discount_vat">0.00</td>
          <td class="currency" tal:content="currency" tal:condition="currency">EUR</td>
        </tr>
      </tal:details>
      <tr class="discount" style="display: none;">
        <td><strong i18n:translate="discount_total">Discount Total:</strong></td>
        <td><strong class="discount_total">Total</strong></td>
        <td tal:condition="currency" class="currency">
          <strong tal:content="currency" class="currency">EUR</strong>
        </td>
      </tr>
      <tr class="shipping" style="display: none;">
        <!--<td>
          <span i18n:translate="summary_shipping">Shipping</span>:
          <span class="shipping_label"></span>
          <span class="shipping_description"></span>:
        </td>
        <td class="cart_shipping">0.00</td>
        <td tal:content="currency" tal:condition="currency">EUR</td>-->
      </tr>
      <tr>
        <td><strong i18n:translate="summary_total">Total:</strong></td>
        <td><strong class="cart_total">Total</strong></td>
        <td class="currency" tal:condition="currency">
          <strong tal:content="currency">EUR</strong>
        </td>
      </tr>
    </table>
    <div class="visualClear"></div>
  </div>

</div>